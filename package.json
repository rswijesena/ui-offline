{
    "name": "ui-offline",
    "version": "0.0.1",
    "description": "ManyWho offline services and components",
    "author": "ManyWho",
    "repository": {
        "type": "git",
        "url": "https://github.com/manywho/ui-offline.git"
    },
    "scripts": {
        "docs": "typedoc js --out docs --ignoreCompilerErrors --mode modules & start docs/index.html",
        "start": "webpack --config webpack.development.config.js --env.build=../ui-html5/build",
        "dist": "webpack --config webpack.production.config.js && node bundle",
        "offline": "node ./js/metaData.js",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.ts": "tslint",
        "*.tsx": [
            "tslint",
            "git add"
        ]
    },
    "devDependencies": {
        "@types/react": "^0.14.57",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^0.28.11",
        "del": "^1.1.1",
        "fs-promise": "^2.0.2",
        "husky": "^0.14.3",
        "less": "^3.0.1",
        "less-loader": "^4.1.0",
        "lint-staged": "^7.0.4",
        "localforage": "^1.5.6",
        "request-promise": "^4.1.1",
        "run-sequence": "^1.2.1",
        "script-loader": "^0.7.0",
        "style-loader": "^0.20.3",
        "ts-loader": "^3.1.1",
        "tslint": "^5.8.0",
        "tslint-loader": "^3.6.0",
        "typedoc": "^0.11.1",
        "typescript": "^2.3.2",
        "uglifyjs-webpack-plugin": "^1.2.4",
        "webpack": "^3.11.0",
        "webpack-bundle-analyzer": "^2.11.1",
        "yargs": "^2.3.0"
    },
    "dependencies": {
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "tslint-config-airbnb": "^5.8.0"
    }
}
